   "source": [
    "#use function to sort out the words into alphatbetical order \n",
    "def alphabet_soup(a):\n",
    "    b = ''.join(sorted(a))\n",
    "    return b\n",
    "\n",
    "#get user input\n",
    "a = input(\"Enter word: \")\n",
    "\n",
    "#call the function\n",
    "b = alphabet_soup(a)\n",
    "\n",
    "#output the result\n",
    "print(b)"
   ]
  }
